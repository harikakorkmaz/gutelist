<table class="table">
			<thead>
				<tr>
					<th>image</th>
					<th>name</th>
				</tr>
			</thead>

			<tbody>
				<% @users.each do |user| %>
				<tr>
					<td class="profileImg"><%= attachment_image_tag(user, :image, :fill, 50, 50) %></td>
					<td><%= user.name %></td>
					<td><%= link_to "Show", user_path(user.id) %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
